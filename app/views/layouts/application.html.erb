<!DOCTYPE html>
<html>
	<head>
	  <title>New Smile Tulsa</title>
	  <%= stylesheet_link_tag :all %>
		
		<%= javascript_include_tag :defaults %>

	  <%= csrf_meta_tag %>
	</head>
	<body>

		<div class="container_12" >	
			<%= render '/layouts/header' %>
			<% if flash[:notice] -%>
				<%= render :partial => '/layouts/flash', :locals => { :notice => flash[:notice] } %>
			<% end -%>
			<%= render '/layouts/navbar' %>
			<div id="main_head" class="grid_10 omega">
				<%= render '/layouts/banner' %>
			  <div id="main" class="grid_8 omega">
					<%= yield %>
				</div>
				<div id="rightbar" class="grid_2  alpha omega">	
					<%= render '/layouts/rightbar' %>
				</div>	
			</div>
			<%= render '/layouts/footer' %>
		</div>

	</body>

</html>
